<h1>商品編集Admin::Items#edit</h1>
<p>Find me in app/views/admin/items/edit.html.erb</p>

<%= form_with madel: @item, url: admin_items_path, method: :patch do |f| %>
 <p>商品画像</p><%= f.file_field :image, accept: "image/*" %>
 <p>商品名</p><%= f.text_field :name %>
 <p>商品説明</p><%= f.text_area :introduction %>
 <p>ジャンル</p><%= @genre.name %>
 <p>税抜価格</p><%= item.price %>
 <p>販売ステータス</p><p>販売中</p><p>販売停止</p>
 <%= f.submit '変更を保存' %>
<% end %>