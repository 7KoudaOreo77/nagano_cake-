<h1>Admin::Items#index</h1>
<p>Find me in app/views/admin/items/index.html.erb</p>

<h4>商品一覧</h4>
<% @items.each do |item| %>
 <div>
  <P>商品ID</P>
   <%= item.id %>
  <P>商品名</P>
   <%= item.name %>
  <P>税抜価格</P>
   <%= item.price %>
  <P>ジャンル</P>
   <%= @genre.name %>

  <p>ステータス</p
   <%= item.status %>
 </div></br>
<% end %>