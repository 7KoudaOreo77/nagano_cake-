<h1>Admin::Genres#index</h1>
<p>Find me in app/views/admin/genres/index.html.erb</p>

<%= form_with model: @genre, url: admin_genres_path do |f| %>
 <p>ジャンル名</p>
 <%= f.text_field :name %>
 <%= f.submit '新規登録' %>
<% end %>

<h4>ジャンル</h4>
<% @genres.each do |genre| %>
<div>
 <%= genre.name %>
 <%= f.submit '編集する', edit_admin_genre_path(params[:id]) %>
</div></br>
<% end %>